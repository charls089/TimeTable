<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeTableView">
        <attr name="row_count" format="integer" />
        <attr name="time_term" format="integer" />
        <attr name="time_width" format="integer" />
        <attr name="header_height" format="dimension" />
        <attr name="box_line_color" format="color" />
        <attr name="box_color" format="color" />
        <attr name="box_height" format="dimension" />
        <attr name="week_type" format="enum">
            <enum name="week" value="0" />
            <enum name="weekend" value="1" />
        </attr>
    </declare-styleable>
</resources>